@article{Neves_Lopes_Pavani_Sales_2021, place={São Paulo, Brasil}, title={A segurança da informação de encontro às conformidades da LGPD}, volume={13}, url={https://fatecpg.edu.br/revista/index.php/ps/article/view/171}, abstractNote={&lt;p&gt;A contínua expansão dos negócios gera diariamente, uma enorme massa de dados, com informações pessoais e organizacionais, públicas ou privadas. Sobre essa massa há uma enorme necessidade de atenção e cuidados, pois transitam qualquer tipo de informação, como fotos, vídeos, relatórios médicos, relatórios policiais, judiciais e informações que devem ser mantidas sob sigilo. Com o intuito de proteger tais informações,  a Lei Geral de Proteção de Dados (LGPD) coíbe  o compartilhamento e a troca de informações pessoais ou institucionais para a manipulação no âmbito empresarial. A garantia da segurança dos dados fica sob a responsabilidade das empresas que realizam a coleta dos mesmos. Este artigo investiga , através do método exploratório, como o tratamento de dados da LGPD pode estar em conformidade com o plano de segurança de informação de uma empresa.&lt;/p&#38;gt;}, journal={Revista Processando o Saber}, author={Neves, Denise Lemes Fernandes and Lopes , Tatiana Schimtz de Almeida and Pavani, Guilheme Cintra and Sales, Rafael Marcos}, year={2021}, month={jun.}, pages={186-198} }

